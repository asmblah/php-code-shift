{
    "name": "asmblah/php-code-shift",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ],
        "psr-4": {
            "Asmblah\\PhpCodeShift\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asmblah\\PhpCodeShift\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "nikic/php-parser": "^4.16"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.2"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
