{
    "name": "asmblah/php-code-shift",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ],
        "psr-4": {
            "Asmblah\\PhpCodeShift\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asmblah\\PhpCodeShift\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "nikic/php-parser": "^4.16",
        "nytris/nytris": "^0.1",
        "psr/log": "^1|^2|^3",
        "symfony/filesystem": "^5.4"
    },
    "require-dev": {
        "mockery/mockery": "1.6.11",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.2"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
